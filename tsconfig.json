{
	"compilerOptions": {
		"lib": [
			"es5",
			"es6"
		],
		"types": ["cypress", "cypress-file-upload"],
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./dist",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": false,
		"esModuleInterop": true
	},
	"exclude": [
		"src/public",
		"node_modules",
		"tests",
		"dist"
	]
}